{
  "name": "daedalus",
  "description": "Daedalus Language Support",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kirides/vscode-daedalus.git"
  },
  "author": {
    "name": "Kirides",
    "url": "https://github.com/Kirides/"
  },
  "displayName": "Daedalus (Gothic/II)",
  "homepage": "https://github.com/Kirides/vscode-daedalus",
  "os": [
    "win32"
  ],
  "cpu": [
    "x64",
    "ia32"
  ],
  "license": "MIT",
  "version": "0.0.8",
  "engines": {
    "vscode": ">=1.48.0"
  },
  "publisher": "Kirides",
  "activationEvents": [
    "onLanguage:daedalus"
  ],
  "contributes": {
    "configurationDefaults": {
      "[daedalus]": {
        "files.encoding": "windows1252"
      }
    },
    "languages": [
      {
        "id": "daedalus",
        "aliases": [
          "Daedalus",
          "daedalus"
        ],
        "extensions": [
          "d",
          "D"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "daedalus",
        "scopeName": "source.daedalus",
        "path": "./syntaxes/daedalus.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "daedalus",
        "path": "./snippets/daedalus.json"
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "compile": "tsc  -p ./",
    "lint": "eslint",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^10.17.29",
    "@types/vscode": "^1.48.0",
    "ts-loader": "^6.2.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@types/glob": "^7.1.3",
    "glob": "^7.1.6",
    "vscode-languageclient": "^6.1.3"
  }
}
